<h1>{{model.address}}</h1>
<h2>Balance: {{model.balance}} {{model.network}}</h2>
<h4>Received Value: {{model.received_value}} {{model.network}}</h4>
<h4>Pending Value: {{model.pending_value}} {{model.network}}</h4>
<h4>Total Transactions: {{model.total_txs}}</h4>
{{!--{{#if model.txs.length}}
  <hr>
  <h2>{{model.txs.length}} Recent Transactions</h2>
  {{#each tx in model.txs}}
  <div class='well'>
    <h5>{{tx.txid}}</h5>
    <h3>Block No. {{tx.block_no}}  Confirmations: {{tx.confirmations}} {{ago tx.time 'X'}}</h3>
    {{#if tx.incoming.value}}
      <h4>Incoming: {{tx.incoming.value}} BTC {{tx.incoming.inputs.length}} Inputs</h4>
    {{/if}}
  </div>
  {{/each}}
{{/if}}--}}
